)\n\n	# Log new script failures\n	[ $batt != 1 ] && [ $failbatt == 0 ] && printf \"battery_status.py \"$batt\" instance(s)\\\\n\" >> sd/battery_status_log.txt\n	[ $serv != 1 ] && [ $failserv == 0 ] && printf \"server_status.py \"$serv\" instance(s)\\\\n\" >> sd/server_status_log.txt\n	\ndone\n",
